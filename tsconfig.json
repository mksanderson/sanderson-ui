{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "ratify": [
        "dist/ratify"
      ],
      "ratify/*": [
        "dist/ratify/*"
      ],
      "sanderson-ui": [
        "dist/sanderson-ui"
      ],
      "sanderson-ui/*": [
        "dist/sanderson-ui/*"
      ]
    }
  }
}